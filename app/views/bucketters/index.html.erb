<p id="notice"><%= notice %></p>

<h1>Listing Bucketters</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bucketters.each do |bucketter| %>
      <tr>
        <td><%= link_to 'Show', bucketter %></td>
        <td><%= link_to 'Edit', edit_bucketter_path(bucketter) %></td>
        <td><%= link_to 'Destroy', bucketter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bucketter', new_bucketter_path %>

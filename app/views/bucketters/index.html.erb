<p id="notice"><%= notice %></p>

<h1>Listing Bucketters</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>
      <td>Name</td>
    </tr>
    <% @bucketters.each do |bucketter| %>
      <tr>
        <td><%= bucketter.name%></td>
        <td><%= link_to 'Show', bucketter %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Sign up', new_bucketter_path %>
